package textbasedapplication;

public class MobilBeraksi {
    public static void main(String[] args) {
        Mobil mobilBramantio = new Mobil();
        
        mobilBramantio.warna = "Stripes";
        mobilBramantio.tahunProduksi = 2019;
        
        System.out.println("Warna : " + mobilBramantio.warna);
        System.out.println("Tahun Produksi : " + mobilBramantio.tahunProduksi );
    }
}
